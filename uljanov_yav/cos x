import math


def cosinus(x, accuracy=0.001):
    result = 0
    term = 1
    n = 0

    while (abs(term)) > accuracy:
        term = (-1) ** n * (x ** (2 * n)) / math.factorial(2 * n)
        result += term
        n += 1
    return result


x_value = float(input())
result = cosinus(x_value)
print(result)
